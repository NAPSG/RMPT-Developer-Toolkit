{
  "name": "ArcGISWebAppBuilder",
  "version": "2.11.0",
  "private": true,
  "scripts": {
    "start": "node server.js",
    "install-windows-service": "winser -i",
    "uninstall-windows-service": "winser -r",
    "test": "mocha --reporter spec --timeout 60s --recursive test/*/test*.js",
    "debug-test": "mocha --debug-brk --reporter spec --timeout 60s test/*/test*.js",
    "jenkins-test": "mocha --reporter mocha-jenkins-reporter --timeout 60s --recursive test/*/test*.js"
  },
  "devDependencies": {
    "mocha": "4.0.1",
    "mocha-jenkins-reporter": "0.3.10",
    "superagent": "3.8.2"
  },
  "dependencies": {
    "bluebird": "3.5.1",
    "body-parser": "1.18.2",
    "compression": "1.7.1",
    "cookie-parser": "1.4.3",
    "ejs": "2.5.7",
    "express": "4.16.2",
    "fs-extra": "5.0.0",
    "jszip": "^3.1.5",
    "log4js": "2.4.1",
    "mongodb": "3.0.0-rc0",
    "multer": "^1.3.0",
    "pako": "1.0.6",
    "request": "2.83.0",
    "requirejs": "2.3.5",
    "restler": "^3.2.2",
    "tingodb": "0.5.1",
    "chokidar": "1.7.0",
    "winser": "1.0.2"
  }
}
